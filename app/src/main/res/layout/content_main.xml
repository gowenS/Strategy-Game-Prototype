<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.rustandtuna.gameprototype.MainActivity"
    tools:showIn="@layout/activity_main"
    android:orientation="horizontal"
    android:gravity="center_horizontal"
    android:background="#2979ff">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center_vertical">

        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/a1Button"
            android:longClickable="false"
            android:layout_margin="5dp" />
        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/b1Button"
            android:layout_margin="5dp" />
        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/c1Button"
            android:longClickable="false"
            android:layout_margin="5dp" />
        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/d1Button"
            android:longClickable="false"
            android:layout_margin="5dp"/>
        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/e1Button"
            android:longClickable="false"
            android:layout_margin="5dp"/>
        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/f1Button"
            android:longClickable="false"
            android:layout_margin="5dp"/>
        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/g1Button"
            android:longClickable="false"
            android:layout_margin="5dp"/>
        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/h1Button"
            android:longClickable="false"
            android:layout_margin="5dp"/>
    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:gravity="center_vertical">

        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/a2Button"
            android:longClickable="false"
            android:layout_margin="5dp"/>

