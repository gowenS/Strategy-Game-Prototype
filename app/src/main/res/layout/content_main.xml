        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/g4Button"
            android:longClickable="false"
            android:layout_margin="5dp"/>
        <com.rustandtuna.gameprototype.gridButton
            android:layout_width="52dp"
            android:layout_height="52dp"
            android:id="@+id/h4Button"
            android:longClickable="false"
            android:layout_margin="5dp"/>

    </LinearLayout>


</LinearLayout>


